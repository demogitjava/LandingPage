<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="de.jgsoftware.landingpage.model.jpa.shopdb.UsersEntity">
        <table name="USERS" schema="PUBLIC" catalog="SHOPDB"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="username">
                <column name="USERNAME"/>
            </basic>
            <basic name="password">
                <column name="PASSWORD"/>
            </basic>
            <basic name="role">
                <column name="ROLE"/>
            </basic>
            <basic name="enabled">
                <column name="ENABLED"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
