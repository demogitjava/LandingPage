<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="de.jgsoftware.landingpage.model.jpa.shopdb.EngProductEntity">
        <table name="ENG_PRODUCT" schema="PUBLIC" catalog="SHOPDB"/>
        <attributes>
            <id name="id">
                <column name="ID"/>
            </id>
            <basic name="productName">
                <column name="productName"/>
            </basic>
            <basic name="productDescription">
                <column name="productDescription"/>
            </basic>
            <basic name="productPrice">
                <column name="productPrice"/>
            </basic>
            <basic name="productUnit">
                <column name="productUnit"/>
            </basic>
            <basic name="image">
                <column name="IMAGE"/>
            </basic>
            <basic name="landingpage">
                <column name="LANDINGPAGE"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
