<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="de.jgsoftware.landingpage.model.jpa.shopdb.AuthoritiesEntity">
        <table name="AUTHORITIES" schema="PUBLIC" catalog="SHOPDB"/>
        <attributes>
            <id name="id">
                <column name="ID"/>
            </id>
            <basic name="username">
                <column name="USERNAME"/>
            </basic>
            <basic name="role">
                <column name="ROLE"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
