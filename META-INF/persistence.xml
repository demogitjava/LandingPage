  -->
  <persistence-unit name="h2demodb">
    <provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>


    <class>de.jgsoftware.landingpage.model.jpa.demodb.Desktoplayout</class>
    <class>de.jgsoftware.landingpage.model.jpa.demodb.Kundenstamm</class>
